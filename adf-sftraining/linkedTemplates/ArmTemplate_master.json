{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-sftraining"
		},
		"ls_kv_sf_training_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-sftraining.vault.azure.net/"
		},
		"ls_sftraining_curation_db_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "curationDB-connectionString"
		},
		"ls_sftraining_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sftrainingstorageaccount.dfs.core.windows.net/"
		},
		"ls_sftraining_golden_db_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "goldenDB-connectionString"
		},
		"ls_sftraining_ingestion_db_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ingestionDB-connectionString"
		},
		"load_payments_every_5min_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"load_payments_every_5min_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-sftraining_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_kv_sf_training_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_sf_training_properties_typeProperties_baseUrl')]"
					},
					"ls_sftraining_curation_db_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_sftraining_curation_db_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_sftraining_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_sftraining_datalake_properties_typeProperties_url')]"
					},
					"ls_sftraining_golden_db_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_sftraining_golden_db_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_sftraining_ingestion_db_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_sftraining_ingestion_db_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-sftraining_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"load_payments_every_5min_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('load_payments_every_5min_properties_pipeline_parameters_windowStart')]"
					},
					"load_payments_every_5min_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('load_payments_every_5min_properties_pipeline_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-sftraining_ArmTemplate_0')]"
			]
		}
	]
}